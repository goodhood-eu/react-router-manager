{
  "name": "react-router-manager",
  "description": "React Router server side rendering with data fetching",
  "author": "Nebenan.de <max@nebenan.de>",
  "license": "MIT",
  "homepage": "https://goodhood.eu",
  "repository": "goodhood-eu/react-router-manager",
  "bugs": "https://github.com/goodhood-eu/react-router-manager/issues",
  "version": "2.3.0",
  "engines": {
    "node": "^10.x.x"
  },
  "keywords": [
    "react",
    "router",
    "route",
    "routing",
    "static routes",
    "route config",
    "react router",
    "server rendering",
    "fetching",
    "data fetching"
  ],
  "main": "lib/index.js",
  "scripts": {
    "reinstall": "rm -rf node_modules package-lock.json && npm install",
    "check": "npm run lint && npm run test && echo \"\\033[0;32mReady to publish\\033[0m\"",
    "clean": "rm -rf ./lib/ || exit 1",
    "compile": "NODE_ENV=production babel src --out-dir lib --copy-files",
    "minify": "find ./lib -name '*.js' -exec terser {} -o {} --config-file terser.json \\;",
    "build": "npm run compile && npm run minify",
    "prepublishOnly": "npm run check && npm run clean && npm run build",
    "lint": "eslint --ext .es,.js ./",
    "lint:tofile": "eslint --ext .es,.js --no-color -o ./eslint-report.log ./",
    "test": "npm run compile && TZ='GMT' mocha \"test/*.js\"",
    "test:tofile": "npm run compile && TZ='GMT' mocha --no-colors -R spec \"test/*.js\" 2>&1 | tee test-report.log"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.0",
    "@babel/core": "^7.6.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-transform-strict-mode": "^7.0.0",
    "@babel/preset-env": "^7.6.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.6.0",
    "babel-plugin-add-module-exports": "^1.0.2",
    "babel-plugin-transform-node-env-inline": "^0.4.3",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "chai": "^4.2.0",
    "eslint": "^6.3.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "mocha": "^6.2.0",
    "react": "^16.9.0",
    "react-router": "^5.0.1",
    "terser": "^4.2.1"
  },
  "peerDependencies": {
    "react": "^16.9.0",
    "react-router": "^5.0.1"
  }
}
